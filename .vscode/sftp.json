{
    "name": "src",
    "remoteExplorer": {
        "filesExclude": [
            "**/**",
            "!wp-config.php",
            "!debug.log",
            "!README.md",
            "!wp-content",
            "!wp-content/README.md",
            "!wp-content/mu-plugins",
            "!wp-content/mu-plugins/README.md",
            "!wp-content/mu-plugins/wp2**",
            "!wp-content/mu-plugins/wp2*/**",
            "!wp-content/plugins",
            "!wp-content/plugins/README.md",
            "!wp-content/plugins/wp2**",
            "!wp-content/plugins/wp2*/**",
            "!wp-content/themes",
            "!wp-content/themes/README.md",
            "!wp-content/themes/wp2",
            "!wp-content/themes/wp2/**",
            "!wp-content/uploads",
            "!wp-content/uploads/README.md"
        ]
    },
    "ignore": [
        "**/**",
        "!README.md",
        "!wp-content",
        "!wp-content/README.md",
        "!wp-content/mu-plugins",
        "!wp-content/mu-plugins/README.md",
        "!wp-content/mu-plugins/wp2**",
        "!wp-content/mu-plugins/wp2*/**",
        "!wp-content/plugins",
        "!wp-content/plugins/README.md",
        "!wp-content/plugins/wp2**",
        "!wp-content/plugins/wp2*/**",
        "!wp-content/themes",
        "!wp-content/themes/README.md",
        "!wp-content/themes/wp2",
        "!wp-content/themes/wp2/**",
        "!wp-content/uploads",
        "!wp-content/uploads/README.md"
    ],
    "defaultProfile": "template",
    "profiles": {
        "template": {
            "remotePath": "/web/template.cloud.wp2s.com/public_html",
            "remote": "wp2-template",
            "uploadOnSave": true,
            "useTempFile": true
        }
    }
}