{
    "name": "src",
    "remoteExplorer": {
        "filesExclude": [
            "**/**",
            "!wp-config.php",
            "!wp-content",
            "!wp-content/mu-plugins",
            "!wp-content/mu-plugins/wp2**",
            "!wp-content/mu-plugins/wp2*/**",
            "!wp-content/plugins",
            "!wp-content/plugins/wp2**",
            "!wp-content/plugins/wp2*/**",
            "!wp-content/themes",
            "!wp-content/themes/wp2",
            "!wp-content/themes/wp2/**"
        ]
    },
    "ignore": [
        "**/**",
        "!wp-content",
        "!wp-content/mu-plugins",
        "!wp-content/mu-plugins/wp2**",
        "!wp-content/mu-plugins/wp2*/**",
        "!wp-content/plugins",
        "!wp-content/plugins/wp2**",
        "!wp-content/plugins/wp2*/**",
        "!wp-content/themes",
        "!wp-content/themes/wp2",
        "!wp-content/themes/wp2/**"
    ],
    "defaultProfile": "template",
    "profiles": {
        "template": {
            "remotePath": "/web/template.cloud.wp2s.com/public_html",
            "remote": "wp2-template",
            "uploadOnSave": true,
            "useTempFile": true
        }
    }
}