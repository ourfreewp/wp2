{
    "$schema": "https://app.blockstudio.dev/schema",
    "apiVersion": 2,
    "name": "wp2/site-search",
    "title": "Site Search",
    "category": "wp2-site",
    "icon": "search",
    "description": "Display a site search.",
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "ariaLabel": true,
        "color": {
            "background": true
        },
        "background": {
            "backgroundImage": true,
            "backgroundSize": true
        },
        "dimensions": {
            "minHeight": true,
            "aspectRatio": true
        },
        "className": false,
        "customClassName": true,
        "spacing": {
            "padding": true,
            "margin": true
        }
    },
    "allowedBlocks": [
        "core/group",
        "core/search"
    ],
    "attributes": {
        "align": {
            "type": "string",
            "default": "full"
        },
        "style": {
            "type": "object",
            "default": {
                "spacing": {
                    "padding": {
                        "top": "0",
                        "bottom": "0",
                        "left": "0",
                        "right": "0"
                    },
                    "margin": {
                        "top": "0",
                        "bottom": "0",
                        "left": "0",
                        "right": "0"
                    }
                }
            }
        }
    },
    "blockstudio": true
}