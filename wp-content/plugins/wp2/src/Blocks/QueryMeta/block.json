{
    "$schema": "https://app.blockstudio.dev/schema",
    "apiVersion": 2,
    "name": "wp2/query-meta",
    "title": "Name",
    "category": "wp2-query",
    "icon": "admin-post",
    "description": "Display the query meta.",
    "supports": {
        "className": false,
        "customClassName": true,
        "align": [
            "left",
            "center",
            "right",
            "full",
            "wide"
        ],
        "typography": {
            "fontSize": true,
            "lineHeight": true,
            "textAlign": true,
            "__experimentalFontFamily": true,
            "__experimentalFontWeight": true,
            "__experimentalLetterSpacing": true
        },
        "renaming": true,
        "color": {
            "background": true,
            "text": true
        }
    },
    "blockstudio": {
        "attributes": [
            {
                "type": "select",
                "label": "Tag",
                "id": "tag",
                "fallback": "div",
                "default": "div",
                "options": [
                    {
                        "label": "H1",
                        "value": "h1"
                    },
                    {
                        "label": "H2",
                        "value": "h2"
                    },
                    {
                        "label": "H3",
                        "value": "h3"
                    },
                    {
                        "label": "H4",
                        "value": "h4"
                    },
                    {
                        "label": "H5",
                        "value": "h5"
                    },
                    {
                        "label": "H6",
                        "value": "h6"
                    },
                    {
                        "label": "P",
                        "value": "p"
                    },
                    {
                        "label": "DIV",
                        "value": "div"
                    },
                    {
                        "label": "SPAN",
                        "value": "span"
                    }
                ]
            }
        ]
    }
}