{
    "$schema": "https://app.blockstudio.dev/schema",
    "apiVersion": 2,
    "name": "wp2/item-meta",
    "title": "Meta",
    "category": "wp2-item",
    "icon": "admin-post",
    "description": "A title block for the queried item.",
    "supports": {
        "className": false,
        "customClassName": true,
        "align": [
            "full",
            "wide"
        ],
        "renaming": true,
        "color": {
            "background": true,
            "text": true
        }
    },
    "blockstudio": {
        "attributes": [
            {
                "type": "select",
                "label": "Tag",
                "id": "tag",
                "fallback": "div",
                "default": "div",
                "options": [
                    {
                        "label": "H1",
                        "value": "h1"
                    },
                    {
                        "label": "H2",
                        "value": "h2"
                    },
                    {
                        "label": "H3",
                        "value": "h3"
                    },
                    {
                        "label": "H4",
                        "value": "h4"
                    },
                    {
                        "label": "H5",
                        "value": "h5"
                    },
                    {
                        "label": "H6",
                        "value": "h6"
                    },
                    {
                        "label": "P",
                        "value": "p"
                    },
                    {
                        "label": "DIV",
                        "value": "div"
                    },
                    {
                        "label": "SPAN",
                        "value": "span"
                    }
                ]
            }
        ]
    }
}